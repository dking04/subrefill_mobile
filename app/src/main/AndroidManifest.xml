<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kingsbyte.subrefill">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Subrefill"
        android:usesCleartextTraffic="true">
        <activity android:name="com.kingsbyte.subrefill.Settings"></activity>
        <activity android:name="com.kingsbyte.subrefill.TvSuccess" />
        <activity android:name="com.kingsbyte.subrefill.Transactions" />
        <activity android:name="com.kingsbyte.subrefill.ElectricitySuccess" />
        <activity android:name="com.kingsbyte.subrefill.DataSuccess" />
        <activity android:name="com.kingsbyte.subrefill.AirtimeSuccess" />
        <activity android:name="com.kingsbyte.subrefill.AddFund" />
        <activity android:name="com.kingsbyte.subrefill.Tv" />
        <activity android:name="com.kingsbyte.subrefill.Electricity" />
        <activity android:name="com.kingsbyte.subrefill.Data" />
        <activity android:name="com.kingsbyte.subrefill.Airtime" />
        <activity android:name="com.kingsbyte.subrefill.Dashboard" />
        <activity android:name="com.kingsbyte.subrefill.ForgotPassword" />
        <activity android:name="com.kingsbyte.subrefill.Register" />
        <activity android:name="com.kingsbyte.subrefill.Login" />
        <activity android:name="com.kingsbyte.subrefill.Splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
        <service android:name=".FirebaseMessageService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />


</manifest>